<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ColorblindResultsActivity">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="301dp"
        android:layout_height="53dp"
        android:text="The test results indicate that you have normal color vision."
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="307dp"
        android:layout_height="144dp"
        android:text="If this result seems right to you, you can move on to the test by pressing &quot;Continue&quot; below. \n\nIf this result seems wrong to you, you can retake the colorblind test by pressing &quot;Redo Colorblind Test&quot;."
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="298dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="525dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Redo Colorblind Test"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="600dp" />

</androidx.constraintlayout.widget.ConstraintLayout>